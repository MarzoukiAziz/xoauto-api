{
    "name": "xoauto-api",
    "version": "1.0.0",
    "description": "Composant Api de XoAuto",
    "private": true,
    "scripts": {
        "build": "node node_modules/typescript/bin/tsc",
        "start": "node dist/service.js",
        "start:prod": "cross-env NODE_ENV=production npm run start",
        "start:recette": "cross-env NODE_ENV=recette npm run start",
        "start:integ": "cross-env NODE_ENV=integ npm run start",
        "prestart:dev": "powershell -File update_conf.ps1",
        "start:dev": "cross-env NODE_ENV=develop node ./node_modules/tsc-watch/index.js --onSuccess \"node dist/service.js\"",
        "mock": "node ./mocks/json-server",
        "test": "mocha -r ts-node/register -r tsconfig-paths/register -r src/tests/hooks.ts \"src/**/*.spec.ts\"",
        "test:watch": "mocha -r ts-node/register -r tsconfig-paths/register -r src/tests/hooks.ts --watch --watch-files \"src/**/*.ts\" \"src/**/*.spec.ts\"",
        "test:coverage": "nyc npm run test",
        "deploy": "npm install && npm run deps && npm run build && npm run test",
        "lint": "eslint src --ext .ts .",
        "lint:fix": "npm run lint -- --fix",
        "deps": "cp -rfp erable-logger-nodejs node_modules",
        "test:cucumber": "cross-env NODE_ENV=test ./node_modules/.bin/cucumber-js --format json:features/report/cucumber_report.json",
        "prepare": "husky install"
    },
    "lint-staged": {
        "!(src/**/*.d.ts)src/**/*.{js,ts}": [
            "prettier --write",
            "eslint",
            "npm run test"
        ]
    },
    "nyc": {
        "extends": "@istanbuljs/nyc-config-typescript",
        "all": true,
        "include": [
            "src/**/*service*.ts",
            "src/utils/*.ts"
        ],
        "exclude": [
            "src/service.ts",
            "src/cntrl/internal-services",
            "src/models/v1",
            "src/cntrl/service-status",
            "src/**/*controller*.ts",
            "dist/**/.js",
            "src/**/*.spec.ts",
            "src/tests/*"
        ],
        "check-coverage": true,
        "reporter": [
            "html",
            "lcov",
            "text",
            "text-summary"
        ],
        "report-dir": "coverage"
    },
    "dependencies": {
        "@babel/cli": "^7.23.4",
        "@babel/core": "^7.23.5",
        "@babel/node": "^7.22.19",
        "@babel/preset-env": "^7.23.5",
        "axios": "^0.26.1",
        "cloudinary": "^1.41.1",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "dotenv": "^16.0.1",
        "express": "~4.18.1",
        "express-session": "^1.17.3",
        "firebase": "^10.7.0",
        "firebase-functions": "^4.5.0",
        "module-alias": "^2.2.2",
        "mongodb": "^6.3.0",
        "mongoose": "^8.0.2",
        "nodemon": "^3.0.1"
    },
    "devDependencies": {
        "@commitlint/cli": "^16.2.3",
        "@commitlint/config-angular": "^16.2.3",
        "@faker-js/faker": "^7.6.0",
        "@istanbuljs/nyc-config-typescript": "^1.0.2",
        "@types/bluebird": "^3.5.36",
        "@types/chai": "^4.3.4",
        "@types/cors": "^2.8.13",
        "@types/dotenv": "^8.2.0",
        "@types/express": "~4.17.13",
        "@types/express-session": "^1.17.5",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/jwt-simple": "^0.5.33",
        "@types/lodash": "~4.14.182",
        "@types/mocha": "^9.1.1",
        "@types/module-alias": "^2.0.1",
        "@types/node": "~17.0.41",
        "@types/properties-reader": "^2.1.1",
        "@types/request": "^2.48.8",
        "@types/semver-compare": "^1.0.1",
        "@types/sinon": "^10.0.13",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.43.0",
        "@typescript-eslint/parser": "^5.58.0",
        "chai": "^4.3.6",
        "chai-http": "^4.3.0",
        "cucumber": "^6.0.7",
        "cucumber-html-reporter": "^5.5.0",
        "eslint": "^8.0.1",
        "eslint-config-prettier": "^8.8.0",
        "eslint-config-standard-with-typescript": "^34.0.1",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-n": "^15.0.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-promise": "^6.0.0",
        "husky": "^7.0.4",
        "lint-staged": "^13.2.0",
        "mocha": "^9.2.2",
        "nodemon": "~2.0.16",
        "nyc": "^15.1.0",
        "prettier": "2.8.7",
        "sinon": "^15.0.1",
        "supertest": "~6.2.3",
        "ts-node": "^10.9.1",
        "tsc-watch": "~5.0.3",
        "tsconfig-paths": "^4.1.2",
        "typescript": "^5.1.6"
    },
    "_moduleAliases": {
        "@root": ".",
        "Models": "dist/models",
        "Controller": "dist/cntrl",
        "Formatters": "dist/formatter",
        "Services": "dist/services",
        "Connectors": "dist/services/connectors",
        "Storage": "dist/services/storage",
        "Interfaces": "dist/interfaces",
        "Constants": "dist/constants",
        "Utils": "dist/utils",
        "Routes": "dist/app/routes",
        "App": "dist/app"
    }
}